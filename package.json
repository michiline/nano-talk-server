{
  "name": "nano-talk-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec npm run babel-node -- ./src/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s --copy-files",
    "build": "npm run clean && npm run build-babel",
    "start-production": "NODE_ENV=production node -r dotenv/config ./build/index.js",
    "run-production": "npm run build && npm run start-production",
    "babel-node": "babel-node"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "7.8.7",
    "@babel/runtime": "7.9.2",
    "body-parser": "1.20.1",
    "dotenv": "8.2.0",
    "express": "4.18.2",
    "mongoose": "5.9.10",
    "nodemon": "2.0.3",
    "uuid": "7.0.3",
    "ws": "7.2.5"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5"
  }
}
